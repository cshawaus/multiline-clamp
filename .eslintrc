{
  "extends": "airbnb",
  "globals": {
    "document": true,
    "window": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": [
    "babel"
  ],
  "rules": {
    "class-methods-use-this": "off",
    "key-spacing": "off",
    "import/prefer-default-export": "off",
    "no-console": "off",
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "object-curly-newline": "off",
    "semi": "off"
  }
}
